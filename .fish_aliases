alias updates "fish ~/fishScripts/update.fish"

alias ga "git add -A"
alias gap "clear;git add -p"
alias gc "git commit -m"
alias gcm "git commit"
alias gs "git status"
alias gl "git log --stat"
alias gp "git push"
alias gac "ga ;and gc"
alias gch "git checkout"
alias gba "git branch -a"
alias gst "git stash"
alias gsl "git stash list"
alias gsp "git stash pop"
alias grm "git rebase master"
alias glog "git log --stat"


alias :q "exit"
alias cl "clear"
alias rmd "rm -rf"
alias vif "vi ~/.fish_aliases"
alias bindings "cat ~/.fish_aliases"
alias epoch "date +%s"

alias gcb "gradle clean build"
alias gb "gradle build"

alias gradlew "./gradlew"
alias buildRun "assemble; gradlew bootRun"
alias pngPurge "rm ~/Desktop/*.png"
