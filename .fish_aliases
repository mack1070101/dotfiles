alias updates "fish ~/fishScripts/update.fish"

alias ga "git add -A"
alias gap "clear;git add -p"
alias gc "git commit -m"
alias gcm "git commit"
alias gs "git status"
alias gp "git push"
alias gac "ga ;and gc"
alias gch "git checkout"
alias gba "git branch -a"
alias gmm "git merge master"
alias gmt "git merge testing"
alias gst "git stash"
alias gsl "git stash list"
alias gsp "git stash pop"

alias :q "exit"
alias cl "clear"
alias rmd "rm -rf"
alias vif "vi ~/.fish_aliases"
alias bindings "cat ~/.fish_aliases"
alias epoch "date +%s"

alias mvnc "mvn -T 8 clean"
alias mvnp "mvn -T 8 package"
alias mvnt "mvn -T 8 test"
alias mvncp "mvn -T 8 clean; mvn -T 8 package"
alias mvnct "mvn -T 8 clean; mvn -T 8 test"
alias mvncd "mvn -T 8 clean; mvn -T 8 deploy"

alias gcb "gradle clean build"
alias gb "gradle build"

alias pngPurge "rm -rf ~/Desktop/*.png"
